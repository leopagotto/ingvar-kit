{
  "name": "leo-workflow-kit",
  "version": "4.1.1",
  "description": "LEO Workflow Kit - AI-powered CLI tool with multi-agent orchestration supporting 4 AI assistants (Copilot, Cursor, Cline, Codeium). Features intelligent task routing, specialized agents (Frontend, Backend, DevOps, Testing, Documentation), spec-first development, GitHub-native issue creation, and automatic GitHub Projects integration",
  "main": "dist/index.js",
  "bin": {
    "leo-workflow": "bin/cli.js",
    "leo": "bin/cli.js"
  },
  "scripts": {
    "start": "node bin/cli.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:model": "jest tests/model-selection",
    "postinstall": "node scripts/postinstall.js",
    "deploy:wiki": "bash scripts/deploy-wiki.sh"
  },
  "keywords": [
    "github",
    "projects",
    "workflow",
    "cli",
    "templates",
    "spec-driven",
    "development",
    "project-management",
    "component-first",
    "best-practices",
    "seo-optimization",
    "performance",
    "frontend",
    "code-quality",
    "automation"
  ],
  "author": "Leo de Souza",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonpagotto/leo-kit.git"
  },
  "bugs": {
    "url": "https://github.com/leonpagotto/leo-kit/issues"
  },
  "homepage": "https://github.com/leonpagotto/leo-kit#readme",
  "files": [
    "bin/",
    "lib/",
    "templates/",
    "scripts/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "lib/model-selection/**/*.js",
      "!lib/model-selection/providers/**",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
